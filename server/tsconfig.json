{
  "compilerOptions": {
  "target": "ES6",
  "module": "CommonJS",
  "rootDir": "./src",
  "outDir": "./dist",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "jsx": "react-jsx", // Add this line to enable JSX parsing
  "resolveJsonModule": true,
  "isolatedModules": true
}

}